  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/styles/site.css">
  <title>{{ title }}</title>
  <meta name = "format-detection" content = "telephone=no" />
  <link rel="icon" href="/images/favicon.ico">
