  <link rel="stylesheet" href="/styles/site.css">
  <script src="/scripts/libs/jquery.js"></script>
  <script src="/scripts/libs/jquery-migrate-1.1.1.js"></script>
  <script src="/scripts/libs/superfish.js"></script>
  <script src="/scripts/libs/jquery.equalheights.js"></script>
  <script src="/scripts/libs/jquery.mobilemenu.js"></script>
  <script src="/scripts/libs/script.js"></script> 
  <script src="/scripts/libs/jquery.ui.totop.js"></script>
  <script src="/scripts/libs/jquery.stellar.js"></script>
  <script src="/scripts/libs/tweens.js"></script>
  <script src="/scripts/libs/jquery-ui-1.10.3.custom.js"></script>
  <script>
  $(window).load(function(){
    $().UItoTop({ easingType: 'easeOutQuart' });

    if ($('html').hasClass('desktop')) {
        $.stellar({
        horizontalScrolling: false,
        verticalOffset: 40
      });
    }

    jQuery( "#accordion" ).accordion({
        collapsible: true,
        heightStyle: "content"
      });          
  }); 
  </script>
